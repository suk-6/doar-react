{
  "name": "@doar/components",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.cjs.js",
  "module": "dist/src/index.js",
  "sideEffects": false,
  "scripts": {
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "typecheck": "tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "rimraf dist *.tsbuildinfo node_modules public storybook-static .rollup.cache",
    "lint": "eslint --ignore-path .eslintignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn format & yarn lint --fix",
    "storybook": "start-storybook -c .storybook -p 20001",
    "build-storybook": "build-storybook -o public",
    "serve-storybook": "npx http-server public",
    "type-build": "tsc",
    "build": "rimraf dist .rollup.cache & cross-env NODE_ENV=production rollup -c"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@rollup/plugin-url": "^7.0.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/rollup": "^6.3.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cross-env": "^7.0.3",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "rollup": "^2.77.2",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0"
  },
  "peerDependencies": {
    "@doar/shared": "^1.0.0",
    "@googlemaps/react-wrapper": "^1.1.35",
    "@googlemaps/typescript-guards": "^2.0.1",
    "@south-paw/react-vector-maps": "^3.1.0",
    "apexcharts": "^3.35.4",
    "chart.js": "^3.8.2",
    "clsx": "^1.2.1",
    "fast-equals": "^4.0.1",
    "leaflet": "^1.8.0",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-chartjs-2": "^4.3.1",
    "react-data-table-component": "^7.5.2",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.0.1",
    "react-portal": "^4.2.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-sparklines": "^1.7.0",
    "react-tabs": "^5.1.0",
    "react-transition-group": "^4.4.4",
    "styled-bootstrap-grid": "^3.1.2",
    "tinycolor2": "^1.4.2"
  }
}
